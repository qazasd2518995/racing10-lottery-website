                    });
                    
                    // ç‚ºæ–°ä½ç½®åˆ›å»ºå±¬æ€§æŠ•æ³¨
                    selectedProperties.forEach(property => {
                        const existingBetIndex = this.selectedBets.findIndex(b => 
                            b.type === 'position' && 
                            b.position === newPosition && 
                            b.property === property
                        );
                        
