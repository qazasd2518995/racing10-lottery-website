                // è·å–æŠ•æ³¨é¡å‹æè¿°
                getBetTypeDesc(betType) {
                    const typeMap = {
                        'champion': 'å† å†›',
                        'runnerup': 'äºšå†›',
                        'number': 'å–®å·',
                        'sumValue': 'å† äºå’Œ',
