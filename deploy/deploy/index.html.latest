                        console.warn('âš ï¸ æ£€æµ‹åˆ°æœƒè©±å·²å¤±æ•ˆï¼Œå¯èƒ½åœ¨å…¶ä»–è£ç½®ç™»å½•');
                        // å¯ä»¥é€‰æ‹©ç«‹å³è·³è½‰åˆ°ç™»å½•é é¢æˆ–é¡¯ç¤ºè­¦å‘Š
                        if (confirm('æ‚¨çš„è´¦å·å·²åœ¨å…¶ä»–è£ç½®ç™»å½•ï¼Œè¯·é‡æ–°ç™»å½•ã€‚')) {
