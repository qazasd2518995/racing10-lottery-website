                getBetTypeLabel(bet) {
                    const typeMap = {
                        'sumValue': 'å† äºå’Œ',
                        'champion': 'å† å†›',
                        'runnerup': 'äºšå†›',
                        'third': 'ç¬¬ä¸‰å',
                        'fourth': 'ç¬¬å››å',
                        'fifth': 'ç¬¬äº”å',
                        'sixth': 'ç¬¬å…­å',
                        'seventh': 'ç¬¬ä¸ƒå',
                        'eighth': 'ç¬¬å…«å',
                        'ninth': 'ç¬¬ä¹å',
                        'tenth': 'ç¬¬åå',
                        'dragonTiger': 'é¾è™',
                        'number': 'å–®å·'
                    };
                    
                    return typeMap[bet.type] || bet.type;
                },
                getBetValueLabel(bet) {
                    const valueMap = {
                        'big': 'å¤§',
                        'small': 'å°',
                        'odd': 'å–®',
                        'even': 'é›™',
                        'dragon': 'é¾',
                        'tiger': 'è™'
                    };
                    
                    if (bet.type === 'number') {
                        return `ç¬¬${bet.position}å ${bet.value}å·`;
                    }
                    
                    if (bet.type === 'dragonTiger') {
                        // é¾è™æŠ•æ³¨å€¼æ ¼å¼å¤„ç†ï¼šdragon_1_10 -> é¾(å† å†›vsç¬¬10å)
                        if (bet.value && bet.value.includes('_')) {
                            const parts = bet.value.split('_');
                            if (parts.length === 3) {
                                const dragonTiger = parts[0] === 'dragon' ? 'é¾' : 'è™';
                                const pos1 = parts[1] === '1' ? 'å† å†›' : parts[1] === '2' ? 'äºšå†›' : `ç¬¬${parts[1]}å`;
                                const pos2 = parts[2] === '10' ? 'ç¬¬åå' : `ç¬¬${parts[2]}å`;
                                return `${dragonTiger}(${pos1}vs${pos2})`;
                            }
                        }
                        return bet.value.startsWith('dragon') ? 'é¾' : 'è™';
                    }
                    
                    if (bet.type === 'position') {
                        // ä½ç½®æŠ•æ³¨é¡¯ç¤ºå±¬æ€§
                        if (bet.property) {
                            return valueMap[bet.property] || bet.property;
                        }
                        // å¦‚æœæœ‰valueï¼Œä¹Ÿå¤„ç†
                        if (bet.value) {
                            return valueMap[bet.value] || bet.value;
                        }
                    }
                    
                    return valueMap[bet.value] || bet.value;
                },
                getBetOdds(bet) {
                    // å„ªå…ˆä½¿ç”¨betå°è±¡ä¸­çš„å¯¦éš›è³ ç‡
                    if (bet.odds && bet.odds > 0) {
                        return bet.odds;
                    }
                    
                    // æ ¹æ“šç”¨æˆ¶ç›¤å£é¡å‹è¨­ç½®é€€æ°´æ¯”ä¾‹å’ŒåŸºç¤è³ ç‡
                    const userMarketType = this.userMarketType || 'D';
                    
                    let rebatePercentage, baseNumberOdds, baseTwoSideOdds;
                    if (userMarketType === 'A') {
                        // Aç›¤é…ç½®ï¼š1.1%é€€æ°´
                        rebatePercentage = 0.011;
                        baseNumberOdds = 10.0;  // Aç›¤åŸºç¤å–®è™Ÿè³ ç‡
                        baseTwoSideOdds = 2.0; // Aç›¤åŸºç¤å…©é¢è³ ç‡ï¼š2Ã—(1-0.011)=1.978
                    } else {
                        // Dç›¤é…ç½®ï¼š4.1%é€€æ°´  
                        rebatePercentage = 0.041;
                        baseNumberOdds = 10.0;  // Dç›¤åŸºç¤å–®è™Ÿè³ ç‡
                        baseTwoSideOdds = 2.0; // Dç›¤åŸºç¤å…©é¢è³ ç‡ï¼š2Ã—(1-0.041)=1.918
                    }
                    
                    if (bet.type === 'number') {
                        return this.odds.number[this.positionToKey(bet.position)] || parseFloat((baseNumberOdds * (1 - rebatePercentage)).toFixed(3));
                    } else if (bet.type === 'dragonTiger') {
                        // é¾è™æŠ•æ³¨å€¼æ ¼å¼ï¼šdragon_1_10, tiger_1_10 ç­‰
                        const dragonTigerType = bet.value.startsWith('dragon') ? 'dragon' : 'tiger';
                        return this.odds.dragonTiger[dragonTigerType] || parseFloat((baseTwoSideOdds * (1 - rebatePercentage)).toFixed(3));
                    } else if (bet.type === 'position') {
                        // ä½ç½®æŠ•æ³¨ä½¿ç”¨æ­£ç¡®çš„èµ”ç‡è®¡ç®—
                        if (['big', 'small', 'odd', 'even'].includes(bet.value) || ['big', 'small', 'odd', 'even'].includes(bet.property)) {
                            return parseFloat((baseTwoSideOdds * (1 - rebatePercentage)).toFixed(3));
                        } else {
                            return bet.odds || 1.0;
                        }
                    } else if (bet.type === 'sumValue') {
                        // å† äºå’Œå€¼æŠ•æ³¨
                        if (['big', 'small', 'odd', 'even'].includes(bet.value)) {
                            return this.odds.sumValue[bet.value] || parseFloat((baseTwoSideOdds * (1 - rebatePercentage)).toFixed(3));
                        } else {
                            // å…·é«”æ•¸å€¼ - ä½¿ç”¨åŒ…å«é€€æ°´çš„èµ”ç‡è¡¨
                            return this.odds.sumValue[bet.value] || parseFloat((7.0 * (1 - rebatePercentage)).toFixed(3));
                        }
                    } else {
                        // å…¶ä»–æŠ•æ³¨é¡å‹ï¼ˆå† å†›ã€äºšå†›ç­‰ï¼‰
                        return this.odds[bet.type]?.[bet.value] || parseFloat((baseTwoSideOdds * (1 - rebatePercentage)).toFixed(3));
                    }
                },
                calculateIndividualPotentialProfit(bet) {
                    const odds = this.getBetOdds(bet);
                    return (bet.amount || 0) * odds;
                },
                calculateTotalPotentialProfit() {
                    return this.selectedBets.reduce((total, bet) => {
                        return total + this.calculateIndividualPotentialProfit(bet);
                    }, 0);
                },
                calculateTotalBetAmount() {
                    return this.selectedBets.reduce((total, bet) => {
