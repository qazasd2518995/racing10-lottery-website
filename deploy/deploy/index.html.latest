                togglePosition(position) {
                    const index = this.selectedPositions.indexOf(position);
                    if (index === -1) {
                        // æ–°å¢ä½ç½®
                        this.selectedPositions.push(position);
                        // è‡ªå‹•ç‚ºæ–°ä½ç½®åˆ›å»ºå·²ç»é¸ä¸­çš„å·ç å’Œå±¬æ€§æŠ•æ³¨
                        this.autoCreateBetsForNewPosition(position);
