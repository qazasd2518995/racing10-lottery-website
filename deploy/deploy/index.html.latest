                        case 'number':
                            return cell.number;
                        case 'size':
                            return cell.isBig ? 'å¤§' : 'å°';
                        case 'parity':
                            return cell.isOdd ? 'å–®' : 'é›™';
                        case 'sum':
                            return cell.sum || '-';
                        case 'sumSize':
                            return cell.sumBig !== undefined ? (cell.sumBig ? 'å¤§' : 'å°') : '-';
                        case 'sumParity':
                            return cell.sumOdd !== undefined ? (cell.sumOdd ? 'å–®' : 'é›™') : '-';
                        case 'dragonTiger':
                            return cell.dragonTiger === 'dragon' ? 'é¾' : 
                                   cell.dragonTiger === 'tiger' ? 'è™' : '-';
                        default:
                            return '';
                    }
