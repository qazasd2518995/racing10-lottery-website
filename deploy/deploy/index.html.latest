                    return typeMap[betType] || 'æœªçŸ¥é¡å‹';
                },
                
                // è·å–æŠ•æ³¨å€¼æè¿°
                getBetValueDesc(bet) {
                    const betType = bet.type || bet.betType;
                    const betValue = bet.value || bet.betValue;
                    
                    if (!betValue) return 'ç„¡';
                    
                    if (betType === 'dragonTiger') {
                        // å¤„ç†é¾è™æŠ•æ³¨æ ¼å¼ï¼šdragon_1_10 -> é¾(å† å†›vsç¬¬10å)
                        if (betValue && betValue.includes('_')) {
                            const parts = betValue.split('_');
                            if (parts.length === 3) {
                                const dragonTiger = parts[0] === 'dragon' ? 'é¾' : 'è™';
                                const pos1 = parts[1] === '1' ? 'å† å†›' : parts[1] === '2' ? 'äºšå†›' : `ç¬¬${parts[1]}å`;
                                const pos2 = parts[2] === '10' ? 'ç¬¬åå' : `ç¬¬${parts[2]}å`;
                                return `${dragonTiger}(${pos1}vs${pos2})`;
                            }
                        }
                        const dragonTigerMap = {
                            'dragon': 'é¾',
                            'tiger': 'è™'
                        };
                        return dragonTigerMap[betValue] || betValue;
                    } else if (['big', 'small', 'odd', 'even'].includes(betValue)) {
                        const valueMap = {
                            'big': 'å¤§',
                            'small': 'å°',
                            'odd': 'å–®',
                            'even': 'é›™'
                        };
                        return valueMap[betValue] || betValue;
                    } else if (betType === 'sumValue' && !isNaN(betValue)) {
                        return `æ€»å’Œå€¼ ${betValue}`;
                    } else if ((betType === 'champion' || betType === 'runnerup') && !isNaN(betValue)) {
                        return `${betValue}å·`;
                    } else if (betType === 'number' && !isNaN(betValue)) {
                        let positionText = '';
                        if (bet.position) {
                            positionText = `ç¬¬${bet.position}å `;
                        }
                        return `${positionText}${betValue}å·`;
                    }
                    
                    return betValue;
