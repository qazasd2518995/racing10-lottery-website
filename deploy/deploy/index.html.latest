                autoCreateBetsForNewPosition(newPosition) {
                    // ç¡®ä¿æŠ•æ³¨é‡‘é¢è‡³å°‘ç‚º1å…ƒ
                    if (!this.betAmount || this.betAmount < 1) {
                        this.betAmount = 1;
