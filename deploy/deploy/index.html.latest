                getPropertyOdds(property) {
                    // é€€æ°´æ¯”ä¾‹ 4.1%
                    const rebatePercentage = 0.041;
                    const baseOdds = {
                        big: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),    // 1.96 Ã— (1-4.1%) = 1.88
                        small: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),
                        odd: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),
                        even: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),
                        dragon: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),
                        tiger: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3))
                    };
                    return baseOdds[property] || parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3));
                },
                
                // ç²å–å¿«é€Ÿé¸é …è³ ç‡ï¼ˆæ”¯æ´Aç›¤Dç›¤å‹•æ…‹è¨ˆç®—ï¼‰
                getQuickOptionOdds(property) {
                    // æ ¹æ“šä½¿ç”¨è€…ç›¤å£é¡å‹å‹•æ…‹è¨ˆç®—è³ ç‡
                    const userMarketType = this.userMarketType || 'D';
                    
                    let rebatePercentage, baseTwoSideOdds;
                    if (userMarketType === 'A') {
                        // Aç›¤é…ç½®ï¼š1.1%é€€æ°´
                        rebatePercentage = 0.011;
                        baseTwoSideOdds = 2.0; // Aç›¤åŸºç¤å…©é¢è³ ç‡ï¼š2Ã—(1-0.011)=1.978
                    } else {
                        // Dç›¤é…ç½®ï¼š4.1%é€€æ°´  
                        rebatePercentage = 0.041;
                        baseTwoSideOdds = 2.0; // Dç›¤åŸºç¤å…©é¢è³ ç‡ï¼š2Ã—(1-0.041)=1.918
                    }
                    
                    // è¨ˆç®—å¯¦éš›è³ ç‡
                    const actualOdds = parseFloat((baseTwoSideOdds * (1 - rebatePercentage)).toFixed(3));
                    
                    // å°æ–¼å¿«é€Ÿé¸é …çš„å¤§å°å–®é›™ï¼Œè¿”å›çµ±ä¸€çš„å…©é¢è³ ç‡
                    if (['big', 'small', 'odd', 'even'].includes(property)) {
                        return actualOdds;
                    }
                    
                    // å¦‚æœæ˜¯å…¶ä»–å±¬æ€§ï¼Œè¿”å›é è¨­å€¼
                    return actualOdds;
                },
                getSumClass(sum) {
                    if (sum > 11) {
                        return 'big';
                    } else if (sum <= 11) {
                        return 'small';
                    }
                    return '';
                },
                getSumLabel(sum) {
                    let labels = [];
                    
                    // å¤§å°
                    if (sum > 11) {
                        labels.push('å¤§');
                    } else {
                        labels.push('å°');
                    }
                    
                    // å–®é›™
                    if (sum % 2 === 1) {
                        labels.push('å–®');
                    } else {
                        labels.push('é›™');
                    }
                    
                    return labels.join('/');
                },
                getSumValueOdds(value) {
                    return this.odds.sumValue[String(value)] || 0;
