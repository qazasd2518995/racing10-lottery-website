                getBetTypeLabel(bet) {
                    const typeMap = {
                        'sumValue': 'å† äºå’Œ',
                        'champion': 'å† è»',
                        'runnerup': 'äºè»',
                        'third': 'ç¬¬ä¸‰å',
                        'fourth': 'ç¬¬å››å',
                        'fifth': 'ç¬¬äº”å',
                        'sixth': 'ç¬¬å…­å',
                        'seventh': 'ç¬¬ä¸ƒå',
