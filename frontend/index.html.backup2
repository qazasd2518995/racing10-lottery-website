                    // é€€æ°´æ¯”ä¾‹ 4.1%
                    const rebatePercentage = 0.041;
                    const baseOdds = {
                        big: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),    // 1.96 Ã— (1-4.1%) = 1.88
                        small: parseFloat((1.96 * (1 - rebatePercentage)).toFixed(3)),
