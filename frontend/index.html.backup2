                // æª¢æŸ¥æ˜¯å¦æœ‰æ–°çµç®—çš„æ³¨å–®
                checkForNewSettlements(oldBets, newBets) {
                    // å¦‚æœæ²’æœ‰èˆŠæ³¨å–®ï¼Œå‰‡ä¸éœ€è¦æ¯”è¼ƒ
                    if (!oldBets || !Array.isArray(oldBets) || oldBets.length === 0) {
