                // ğŸï¸ å‰µå»ºç«¶è³½ä¸­çš„è³½è»Š
                createCompetingCars(container) {
                    const trackHeight = 400;
                    const carCount = 10;
                    const spacing = trackHeight / carCount;
                    const containerWidth = container.offsetWidth || 1200;
                    
                    this.racingCars = [];
                    
                    for (let i = 1; i <= carCount; i++) {
